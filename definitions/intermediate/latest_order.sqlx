config {
    type: "table",
    database: "doctor-ilcsi",
    schema: "dwh_crossmall",
    name: "latest_order",
}


SELECT * except(rn, partition_date)
FROM (
    SELECT *,
           ROW_NUMBER() OVER (PARTITION BY order_number, line_no ORDER BY updated_at DESC) AS rn
    FROM ${ref('order')}
) AS subquery
WHERE rn = 1
